[32m+[m
[32m+[m[32mfrom mptt.admin import DraggableMPTTAdmin[m
[32m+[m[32mfrom .models import Category, Article, Comment, ViewCount[m
[32m+[m
[32m+[m[32m@admin.register(Category)[m
[32m+[m[32mclass CategoryAdmin(DraggableMPTTAdmin):[m
[32m+[m[32m    """[m
[32m+[m[32m    –ê–¥–º–∏–Ω-–ø–∞–Ω–µ–ª—å –º–æ–¥–µ–ª–∏ –∫–∞—Ç–µ–≥–æ—Ä–∏–π[m
[32m+[m[32m    """[m
[32m+[m[32m    list_display = ('tree_actions', 'indented_title', 'id', 'title', 'slug')[m
[32m+[m[32m    list_display_links = ('title', 'slug')[m
[32m+[m[32m    prepopulated_fields = {'slug': ('title',)}[m
[32m+[m
[32m+[m[32m    fieldsets = ([m
[32m+[m[32m        ('–û—Å–Ω–æ–≤–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è', {'fields': ('title', 'slug', 'parent')}),[m
[32m+[m[32m        ('–û–ø–∏—Å–∞–Ω–∏–µ', {'fields': ('description',)})[m
[32m+[m[32m    )[m
[32m+[m
[32m+[m[32m@admin.register(Article)[m
[32m+[m[32mclass ArticleAdmin(admin.ModelAdmin):[m
[32m+[m[32m    prepopulated_fields = {'slug': ('title',)}[m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(Comment)[m
