[32m+[m[32mfrom django.contrib.syndication.views import Feed[m
[32m+[m[32mfrom django.urls import reverse[m
[32m+[m[32mfrom .models import Article[m
[32m+[m
[32m+[m[32mclass LatestArticlesFeed(Feed):[m
[32m+[m[32m    title = "–í–∞—à —Å–∞–π—Ç - –ø–æ—Å–ª–µ–¥–Ω–∏–µ —Å—Ç–∞—Ç—å–∏"[m
[32m+[m[32m    link = "/feeds/"[m
[32m+[m[32m    description = "–ù–æ–≤—ã–µ —Å—Ç–∞—Ç—å–∏ –Ω–∞ –º–æ–µ–º —Å–∞–π—Ç–µ."[m
[32m+[m
[32m+[m[32m    def items(self):[m
[32m+[m[32m        return Article.objects.order_by('-time_update')[:5][m
[32m+[m
[32m+[m[32m    def item_title(self, item):[m
[32m+[m[32m        return item.title[m
[32m+[m
[32m+[m[32m    def item_description(self, item):[m
[32m+[m[32m        return item.short_description[m
[32m+[m
[32m+[m[32m    def item_link(self, item):[m
[32m+[m[32m        return reverse('blog:articles_detail', args=[item.slug])[m
